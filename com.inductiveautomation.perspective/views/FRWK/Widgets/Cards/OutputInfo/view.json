{
  "custom": {},
  "params": {
    "image": "leafy/sun",
    "paramsName": "@ui.params.lightQuantity@",
    "paramsValue": "value",
    "textColor": "#000000"
  },
  "propConfig": {
    "params.image": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.paramsName": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.paramsValue": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.textColor": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 200
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "ParameterValue"
            },
            "position": {
              "basis": "100px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.paramsValue"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "fontSize": "30px",
                "fontWeight": "bolder",
                "textAlign": "right"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "100px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "path": "view.params.image"
                  },
                  "type": "property"
                }
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "TopContainer"
        },
        "position": {
          "basis": "200px"
        },
        "props": {
          "style": {
            "overflow": "hidden"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "ParameterName"
            },
            "position": {
              "basis": "32px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.paramsName"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "fontSize": "25px",
                "fontWeight": "bolder",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "BottomContainer"
        },
        "position": {
          "basis": "200px"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "propConfig": {
      "props.style.color": {
        "binding": {
          "config": {
            "path": "view.params.textColor"
          },
          "type": "property"
        }
      }
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}