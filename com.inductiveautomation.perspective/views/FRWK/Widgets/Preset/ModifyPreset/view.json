{
  "custom": {
    "tagPath": [
      "[default]Presets/18/Description"
    ],
    "value": "Preset di test"
  },
  "params": {
    "presetId": 18
  },
  "propConfig": {
    "custom.tagPath": {
      "persistent": true
    },
    "custom.value": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "0": "{view.custom.tagPath[0]}"
          },
          "tagPath": "{0}"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.presetId": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "name": "TagBrowseTree"
        },
        "position": {
          "basis": "520px"
        },
        "propConfig": {
          "props.root.path": {
            "binding": {
              "config": {
                "path": "view.params.presetId"
              },
              "transforms": [
                {
                  "code": "\treturn \u0027[default]Presets/\u0027+ str(value)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.selection.values": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.custom.tagPath"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "root": {},
          "selection": {
            "mode": "single"
          }
        },
        "type": "ia.display.tag-browse-tree"
      },
      {
        "meta": {
          "name": "DateTimePicker"
        },
        "position": {
          "basis": "360px"
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "path": "view.custom.value"
              },
              "transforms": [
                {
                  "code": "\ttry:\n\t\tsystem.date.toMillis(value)\n\t\treturn True\n\texcept:\n\t\treturn False",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.locale": {
            "binding": {
              "config": {
                "path": "session.props.locale"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "path": "view.custom.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "formattedValue": null
        },
        "type": "ia.input.date-time-picker"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}