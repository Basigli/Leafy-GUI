          }
        },
        "props": {
          "direction": "column",
          "path": "FRWK/Widgets/OverviewDock/ParameterController"
        },
        "type": "ia.display.flex-repeater"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
