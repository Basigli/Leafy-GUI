{
  "custom": {
    "UVLight": true,
    "airHumidity": "50",
    "airTemperature": "40",
    "irrigationPump": false,
    "isTankEmpty": 0,
    "lightQuantity": "20",
    "name": "Test MQTT",
    "terrainHumidity": "57",
    "ventilation": false
  },
  "params": {
    "id": 1
  },
  "propConfig": {
    "custom.UVLight": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Actuators/UVLight"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.airHumidity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Sensors/AirHumidity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "#",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.airTemperature": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Sensors/AirTemperature"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "#",
