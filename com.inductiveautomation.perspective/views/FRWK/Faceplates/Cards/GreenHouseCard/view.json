{
  "custom": {
    "UVLight": false,
    "airHumidity": "60",
    "airTemperature": "26",
    "irrigationPump": true,
    "isTankEmpty": false,
    "lightQuantity": "63",
    "name": "Serra 1",
    "terrainHumidity": "60",
    "ventilation": false
  },
  "params": {
    "id": 1
  },
  "propConfig": {
    "custom.UVLight": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/UVLight"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.airHumidity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/AirHumidity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "#",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.airTemperature": {
