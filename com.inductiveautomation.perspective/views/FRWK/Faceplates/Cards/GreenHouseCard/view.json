    "custom.isTankEmpty": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Sensors/IsTankEmpty"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.lightQuantity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Sensors/LightQuantity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "#",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.name": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Info/Name"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.terrainHumidity": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Sensors/TerrainHumidity"
        },
        "transforms": [
          {
            "formatType": "numeric",
            "formatValue": "#",
            "type": "format"
          }
        ],
        "type": "tag"
      },
      "persistent": true
    },
    "custom.ventilation": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Actuators/Ventilation"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.id": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 300,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Title"
            },
            "position": {
              "basis": "170px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.name"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "fontSize": "25px",
                "fontWeight": "bolder",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Header"
        },
        "position": {
          "basis": "78px"
        },
        "props": {
          "alignItems": "center",
          "justify": "center"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "FlexRepeater"
            },
            "position": {
              "basis": "320px"
            },
            "propConfig": {
              "props.instances[0].value": {
                "binding": {
                  "config": {
                    "path": "view.custom.airTemperature"
                  },
                  "transforms": [
                    {
                      "code": "\treturn str(value) + u\u0027Â°\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.instances[1].value": {
                "binding": {
                  "config": {
                    "path": "view.custom.irrigationPump"
                  },
                  "transforms": [
                    {
                      "code": "\treturn \u0027@ui.state.active@\u0027 if value else \u0027@ui.state.notActive@\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.instances[2].value": {
                "binding": {
                  "config": {
                    "path": "view.custom.ventilation"
                  },
                  "transforms": [
                    {
                      "code": "\treturn \u0027@ui.state.active@\u0027 if value else \u0027@ui.state.notActive@\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.instances[3].value": {
                "binding": {
                  "config": {
                    "path": "view.custom.lightQuantity"
                  },
                  "transforms": [
                    {
                      "code": "\treturn str(value) + u\u0027%\u0027",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "alignContent": "center",
              "alignItems": "center",
              "elementPosition": {
                "basis": "120px",
                "grow": 0,
                "shrink": 0
              },
              "instances": [
                {
                  "icon": "leafy/thermostat",
                  "iconColor": "#C93F49",
                  "instancePosition": {},
                  "instanceStyle": {
                    "classes": ""
                  }
                },
                {
                  "icon": "leafy/water_pump",
                  "iconColor": "#002060",
                  "instancePosition": {},
                  "instanceStyle": {
                    "classes": ""
                  }
                },
                {
                  "icon": "leafy/air",
                  "iconColor": "#DAE3F3",
                  "instancePosition": {},
                  "instanceStyle": {
                    "classes": ""
                  }
                },
                {
                  "icon": "leafy/sun",
                  "iconColor": "#F4F183",
                  "instancePosition": {},
                  "instanceStyle": {
                    "classes": ""
                  }
                }
              ],
              "justify": "center",
              "path": "FRWK/Widgets/Cards/CardInfo",
              "useDefaultViewHeight": false,
              "useDefaultViewWidth": false,
              "wrap": "wrap"
            },
            "type": "ia.display.flex-repeater"
          }
        ],
        "meta": {
          "name": "Content"
        },
        "position": {
          "basis": "200px",
          "grow": 1
        },
        "type": "ia.container.flex"
      }
    ],
    "events": {
      "dom": {
        "onClick": {
          "config": {
            "params": {
              "greenHouseId": "{view.params.id}"
            },
            "view": "FRWK/App/GreenHouse/GreenHouseOverview"
          },
          "scope": "C",
          "type": "nav"
        }
      }
    },
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "background": "linear-gradient(145deg, #b9a38f, #dbc2aa)",
        "borderBottomLeftRadius": "60px",
        "borderBottomRightRadius": "60px",
        "borderStyle": "none",
        "borderTopLeftRadius": "60px",
        "borderTopRightRadius": "60px",
        "boxShadow": "20px 20px 60px #ae9a87,\n             -20px -20px 60px #ecd0b7",
        "color": "#62584E",
        "cursor": "pointer"
      }
    },
    "type": "ia.container.flex"
  }
}