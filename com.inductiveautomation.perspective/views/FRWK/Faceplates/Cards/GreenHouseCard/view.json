        "type": "tag"
      },
      "persistent": true
    },
    "custom.ventilation": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "id": "{view.params.id}"
          },
          "tagPath": "[default]GreenHouses/{id}/Ventilation"
        },
        "type": "tag"
      },
      "persistent": true
